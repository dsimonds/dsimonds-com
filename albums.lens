
<koken:include file="header.lens" />

<koken:load>
  <ol class="grid">
    <koken:loop>
      <li>
        <koken:link>
          <koken:img size="3:2" />
        </koken:link>
        <h2>
          <koken:link>
            {{ album.title }}
          </koken:link>
        </h2>
      </li>
    </koken:loop>
  </ol>
<koken:else>
  <koken:note>
    No public albums found
  </koken:note>
</koken:load>

<koken:include file="footer.lens" />
